{
	"name": "@w3s/sveltekit",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "bun scripts/get-local-ip.js && vite dev",
		"build:app:css": "bunx postcss-cli src/app.css -o static/app.css",
		"build:css": "bun run build:app:css",
		"build": "bun run build:css && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@reown/appkit": "^1.0.0",
		"@reown/appkit-adapter-wagmi": "^1.0.0",
		"@scaffold-ui/components": "^0.1.3",
		"@scaffold-ui/debug-contracts": "^0.1.2",
		"@scaffold-ui/hooks": "^0.1.2",
		"@tanstack/svelte-query": "^5.59.15",
		"@types/qrcode": "^1.5.6",
		"@uniswap/sdk-core": "^4.0.7",
		"@uniswap/v2-sdk": "^3.0.1",
		"@wagmi/core": "^2.16.4",
		"abitype": "^1.0.6",
		"bits-ui": "^1.0.0-next.98",
		"blo": "^2.0.0",
		"clipboard": "^2.0.11",
		"clsx": "^2.1.1",
		"qrcode": "^1.5.4",
		"shadcn-svelte": "^1.0.0-next.19",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"type-fest": "^4.26.1",
		"viem": "^2.34.0",
		"wagmi": "^2.16.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/cli": "^4.1.18",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/clipboard": "^2.0.10",
		"@types/node": "^20.0.0",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.45",
		"postcss-import": "^16.1.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.3",
		"typescript": "^5.9.3",
		"vite": "^7.2.2"
	},
	"packageManager": "bun@latest"
}
